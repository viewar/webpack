{
  "name": "@viewar/webpack",
  "version": "1.6.0",
  "description": "shared webpack config of ViewAR",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf bundle",
    "start": "cross-env WEBPACK_PATH=test webpack-dev-server --config ./webpack.config.js --open --mode development --env development_mock",
    "build": "cross-env WEBPACK_PATH=test NODE_ENV=production webpack -p --config ./webpack.config.js --mode production --env production",
    "test": "npm run lint && npm run karma && npm run build",
    "karma": "cross-env WEBPACK_PATH=test karma start ./src/test/karma.config.js",
    "lint": "prettier **/*.{css,scss,md,json} --write && cross-env WEBPACK_PATH=test eslint --fix \"**/*.*{js,jsx}\" --ignore-pattern \"build/**/*\" --format pretty"
  },
  "homepage": "https://viewar.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/viewar/webpack"
  },
  "bugs": {
    "url": "https://github.com/viewar/webpack/issues"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "viewar",
    "webpack",
    "config",
    "shared"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "esmodules": true,
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-default-from"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env WEBPACK_PATH=test lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix --format pretty"
    ]
  },
  "author": "Stefan Friedl <sf@viewar.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/register": "^7.5.5",
    "@formatjs/intl-pluralrules": "^1.3.1",
    "@formatjs/intl-relativetimeformat": "^4.4.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "express-useragent": "^1.0.13",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-port-check": "^2.0.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass": "^1.23.0-module.beta.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "viewar-core": "^11.41.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@viewar/config-eslint": "^1.8.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^6.0.3",
    "expect": "^24.9.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "puppeteer": "^2.0.0"
  }
}
